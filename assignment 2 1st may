s={}
n=int(input())
for i in range(1,n+1):
    l=[]
    sno=int(input('Enter student number'))
    sname=input('Enter student name')
    sgrp=input('Enter student group')
    sclg=input('Enter student college')
    sph=int(input('Enter student phone number'))
    l.append(sname)
    l.append(sgrp)
    l.append(sclg)
    l.append(sph)
    s[sno]=l
print(s)
