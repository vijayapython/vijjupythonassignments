l=list(map(int,input().split()))
d={}
for i in l:
    if i not in d:
        d[i]=1
    else:
        d[i]=d[i]+1
m=list(d.values())
r=min(m)
s=max(m)
for i in d:
    if d[i]==r:
        min=i
        print('min:',min)
    if d[i]==s:
        max=i
        print('max: ',max)
if max>min:
     print('diff: ',max-min)
else:
    print('diff:Â ',min-max)
