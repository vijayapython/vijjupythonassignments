n=int(input())
l=list(map(int,input().split()))
x=int(input())
 low=0
 high=n-1
mid=(low+high)//2 
while high>=low:
 if x>l[mid]:
  low=mid+1
elif x==l[mid]:
 flag=1
  break
else:
 high=mid-1
  mid=(low+high)//2
if flag==1:
  print('element is found at',mid,'index location')
 else:
  print('element not found')
